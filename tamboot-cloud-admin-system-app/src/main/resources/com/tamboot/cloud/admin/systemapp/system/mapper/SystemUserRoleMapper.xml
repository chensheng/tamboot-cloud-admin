<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tamboot.cloud.admin.systemapp.system.mapper.SystemUserRoleMapper">
    <resultMap type="com.tamboot.cloud.admin.systemapp.system.model.SystemUserRoleModel" id="Model"></resultMap>

    <delete id="deleteByRoleCode">
        delete from system_user_role where role_code = #{roleCode}
    </delete>

    <delete id="deleteByUserId">
        delete from system_user_role where user_id = #{userId}
    </delete>

</mapper>